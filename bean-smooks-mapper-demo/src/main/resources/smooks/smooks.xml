<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
                      xmlns:core="http://www.milyn.org/xsd/smooks/smooks-core-1.3.xsd"
                      xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.2.xsd">

    <core:filterSettings type="SAX" />


    <jb:bean beanId="lineItem" class="com.example.entity.UserTar" createOnElement="com.example.entity.UserSrc">
        <jb:value property="uuidTar" data="uuidSrc" />
        <jb:value property="userNameTar" data="userNameSrc" />
        <jb:value property="passwordTar"  data="passwordSrc" />
    </jb:bean>

</smooks-resource-list>