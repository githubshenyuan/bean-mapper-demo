            <b>userSrc.uuidSrc</b>
        </field>
        <field>
            <a>userNameSrc</a>
            <b>userSrc.userNameSrc</b>
        </field>
        <field>
            <a>passwordSrc</a>
            <b>userSrc.passwordSrc</b>
        </field>
        <field>
            <a >uuidTar</a>
            <b>userTar.uuidTar</b>
        </field>
        <field>
            <a>userNameTar</a>
            <b>userTar.userNameTar</b>
        </field>
        <field>
            <a>passwordTar</a>
            <b>userTar.passwordTar</b>
        </field>
    </mapping>

</mappings>